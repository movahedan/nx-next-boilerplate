{
  "files": [],
  "include": [],
  "exclude": ["node_modules", "tmp"],
  "compileOnSave": false,
  "compilerOptions": {
    "module": "es2020",
    "moduleResolution": "node",
    "target": "es2015",
    "lib": ["es2021"],
    "rootDir": ".",
    "outDir": "./out/tsc",
    "types": [],
    "baseUrl": ".",
    "paths": {
      "@nx-starter/ui": ["libs/ui/src/index.ts"],
      "@nx-starter/modules/analytics": ["libs/modules/analytics/src/index.ts"],
      "@nx-starter/modules/fetcher": ["libs/modules/fetcher/src/index.ts"],
      "@nx-starter/modules/media-queries": ["libs/modules/media-queries/src/index.ts"],
      "@nx-starter/utilities-shared": ["libs/utilities-shared/src/index.ts"],
      "@nx-starter/utilities-react": ["libs/utilities-react/src/index.ts"],
      "@nx-starter/utilities-next": ["libs/utilities-next/src/index.ts"]
    },
    "strict": true,
    "allowJs": false,
    "importHelpers": true,
    "sourceMap": true,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "stripInternal": true,
    "preserveConstEnums": true,
    "downlevelIteration": false,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noFallthroughCasesInSwitch": true,
    "noPropertyAccessFromIndexSignature": true,
    "importsNotUsedAsValues": "error",
    "exactOptionalPropertyTypes": true,
    "forceConsistentCasingInFileNames": true
  }
}
